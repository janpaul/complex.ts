{
  "name": "apt",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "bin": "build/index.js",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "jest": "^26.6.3",
    "pkg": "^4.4.9",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "babel --extensions \".ts\" src -d build",
    "bundle": "pkg -t node14 -o apt .",
    "format": "prettier --write .",
    "start": "yarn build && node build/index.js",
    "test": "jest"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "data/**/*"
  }
}
